<!doctype html>
<html class="two-columns" data-theme="dark">
<head>
  {{> head }}
</head>
<body>
  <div id="docs" data-level>
    {{> header}}

    <div class='content-root'>
      <div class='menubar'>
	      {{> deviceselector}}
        <div class="navMenu" data-swiftype-index="false">
          {{{navigation}}}
        </div>
      </div>
      <div class="page-body">
        {{> document-search}}
        <div class='content-inner'>
          <div class="content" >
            {{{contents}}}
          </div>
        </div>
      </div>
    </div>
    {{> details-overlay}}
    {{> image-overlay}}
    {{> search-overlay }}
    {{> scripts}}
</body>
</html>
