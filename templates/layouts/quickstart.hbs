<!doctype html>
<html class="two-column">
<head>
  {{> head sectionTitle='quick start'}}
</head>
<body>
  <div id="docs" data-level>
    {{> header}}

    <div class='content-root'>
      <div class='menubar'>
      <ul class="nav guide-menu">
        {{#each quickstart}}
           <ul>
              <li class="top-level {{#page-equal ../path.href ../path.name path.href path.name}}active{{/page-equal}}">
                {{#page-equal ../path.href ../path.name path.href path.name}}
                  <span>{{{title}}}</span>
                  {{#if ../../toc}}
                    <i id="toggle-in-page-nav" class="ion-plus"></i>
                  {{/if}}
                {{else}}
                  <a href="{{path.href}}{{path.name}}">{{{title}}}</a>
                {{/page-equal}}
              </li>
                {{#page-equal ../path.href ../path.name path.href path.name}}
                  {{#if ../../toc}}
                    <div class="in-page-toc-container">
                      <ul class="nav in-page-toc hide">
                        {{#each ../../../toc}}
                        <li class="middle-level" data-nav=""><a href="#{{id}}">{{{text}}}</a></li>
                        {{/each}}
                      </ul>
                    </div>
                  {{/if}}
                {{/page-equal}}
            </ul>
        {{/each}}
      </ul>
      </div>
      <div class="page-body">
        <div class='content-inner'>
          <div class="content" >
            {{{contents}}}
          </div>
        </div>
      </div>
    </div>
    {{> scripts}}


</body>
</html>
