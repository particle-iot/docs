<div class="apiHelperCloudApiListCustomers">
    <div class="apiHelper">
        <div class="apiHelperBox">
            <table class="apiHelperTableNoMargin">
                <tr><td class="apiHelperProductSelectorLabel">Product Access Token</td><td><input type="text" size="50" class="apiHelperAuthSettingsProductAccessToken" value=""></td><td>&nbsp;</td></tr>
                <tr><td class="apiHelperProductSelectorLabel">Product ID</td><td><input type="text" size="10" class="apiHelperAuthSettingsProduct" value=""></td><td>&nbsp;</td></tr>
                <tr><td class="apiHelperProductSelectorLabel">Page</td><td><input type="text" size="10" class="apiHelper_page" value=""></td><td class="apiHelperProductSelectorLabel">(starts at 1, optional)</td></tr>
                <tr><td class="apiHelperProductSelectorLabel">Per Page</td><td><input type="text" size="10" class="apiHelper_perPage" value=""></td><td class="apiHelperProductSelectorLabel">(25 if not set, optional)</td></tr>
                <tr><td colspan="3"><button class="apiHelperActionButton">List Customers</button></td></tr>
            </table>

            <div class="apiHelperStatus" style="padding-top: 5px;"></div>
            <div class="apiHelperApiRequest" style="padding-top: 8px; display:none">Request:<br/><pre class="apiHelperMonoSmall"></pre></div>
            <div class="apiHelperApiResponse" style="display:none">Response:<br/><pre class="apiHelperMonoSmall"></pre></div>
        </div>
    </div>
    <div class="apiHelperCloudApiOutputJson" style="height:{{height}}px; overflow:scroll; display:none">
        <pre class="codeboxPre"><code class="codebox lang-json">                
        </code></pre>
    </div>
</div>

