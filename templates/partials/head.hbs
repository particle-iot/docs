<meta charset='utf-8'>
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0, width=device-width">
<meta name="color-scheme" content="dark light">

{{#if noIndex}}
<meta name="robots" content="noindex">
{{/if}}

{{#if sectionTitle}}
{{#if titlePlatform}}
<title>{{titlePlatform}} | {{sectionTitle}} | Particle</title>
{{else}}
<title>{{title}} | {{sectionTitle}} | Particle</title>
{{/if}}
{{else}}
<title>Particle {{title}}</title>
{{/if}}


<!-- Spark theme mods -->
<link href='/assets/css/style.css' rel='stylesheet'>
<link rel="stylesheet" href="https://cdn.icomoon.io/49057/Dashboard/style-cf.css?vut8cd">

{{#if canonical}}
<link rel="canonical" href="{{canonical}}/" />
{{/if}}

<!-- Meta -->
{{#if sectionTitle}}
{{#if titlePlatform}}
<meta content="{{titlePlatform}} | {{sectionTitle}}" property="og:title">
{{else}}
<meta content="{{title}} | {{sectionTitle}}" property="og:title">
{{/if}}
{{else}}
<meta content="{{title}}" property="og:title">
{{/if}}

{{#if description}}
<meta content="{{description}}" name="description">
{{else}}
<meta content="Documentation for Particle, a platform for connected devices." name="description">
{{/if}}

<!-- Favicon -->
<link rel="shortcut icon" href="/assets/favicon3.ico" type="image/x-icon" />

<!-- Google Site Verification -->
<meta name="google-site-verification" content="xoBkwTJRCA1Uxlx-g3dDw3YeaXTTA5hXQJfmR1mL84s" />

<!-- jQuery -->
<script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>


{{#contains includeDefinitions 'codemirror'}}
<link rel="stylesheet" href="/assets/files/codemirror/codemirror.css">
<link rel="stylesheet" href="/assets/files/codemirror/lint.css">
{{/contains}}

