{{#prettify condense='true' indent=0}}
{{#with group}}

	{{#each this}}
		{{permalink-header title}}
		<pre class='api-code api-definition' data-title='Definition'><code class='prettyprint lang-json'>{{uppercase type}} {{url}}</code></pre>
		{{{description}}}


        {{#if header.fields}}
			<ul class='api-attributes api-header' data-title='Request Headers'>
				{{#each header.fields.Header}}
					<li>
						<div class='api-field'>{{field}}{{#unless optional}}<span class='field-required'>REQUIRED</span>{{/unless}}
						<small>{{#if allowedValues}}{{join allowedValues ','}}{{else}}{{type}}{{/if}}</small></div>
						<div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
					</li>
				{{/each}}
			</ul>
        {{/if}}

		{{#if permission}}
			<ul class='api-attributes api-header' data-title='Required Scope'>
				{{#each permission}}
					<li>
						<div class='field-desc'><code>{{{name}}}</code></div>
					</li>
				{{/each}}
			</ul>
        {{/if}}

		{{#if parameter.fields}}
			<ul class='api-attributes api-request' data-title='Request Arguments'>
				{{#each parameter.fields.Parameter}}
					<li>
						<div class='api-field'>{{field}}{{#unless optional}}<span class='field-required'>REQUIRED</span>{{/unless}}
						<small>{{#if allowedValues}}{{join allowedValues ','}}{{else}}{{type}}{{/if}}</small></div>
						<div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
					</li>
				{{/each}}
			</ul>
		{{/if}}

		{{#each examples}}
<pre class='api-code api-request-example' data-title='{{#starts-with content title}}Example Request{{else}}{{title}}{{/starts-with}}'><code class='prettyprint lang-{{this.type}}'>{{{this.content}}}</code></pre>
		{{/each}}

		{{#each success.fields}}
			<ul class='api-attributes api-response' data-title='Response Attributes'>
				{{#each this}}
					<li>
						<div class='api-field'>{{field}}<small>{{type}}</small></div>
						<div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
					</li>

					{{#if this.nestedParams}}
						<li class='nested-wrapper'>
							<ul class='nested-params'>
								{{#each this.nestedParams}}
								<li class='nested-param'>
								<div class='api-field'>{{field}}<small>{{type}}</small></div>
								<div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
								</li>
								{{/each}}
							</ul>
						</li>
					{{/if}}
				{{/each}}
			</ul>
		{{/each}}

		{{#each success.examples}}
<pre class='api-code api-response-example' data-title='{{#starts-with content title}}Example Response{{else}}{{title}}{{/starts-with}}'><code class='prettyprint lang-{{type}}'>{{{content}}}</code></pre>
		{{/each}}

        {{#each error.fields}}
            <ul class='api-attributes api-response' data-title='Error Attributes'>
                {{#each this}}
                    <li>
                        <div class='api-field'>{{field}}<small>{{type}}</small></div>
                        <div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
                    </li>

                    {{#if this.nestedParams}}
                        <li class='nested-wrapper'>
                            <ul class='nested-params'>
                                {{#each this.nestedParams}}
                                    <li class='nested-param'>
                                        <div class='api-field'>{{field}}<small>{{type}}</small></div>
                                        <div class='field-desc'>{{#if description}}{{{description}}}{{else}}&mdash;{{/if}}</div>
                                    </li>
                                {{/each}}
                            </ul>
                        </li>
                    {{/if}}
                {{/each}}
            </ul>
        {{/each}}

        {{#each error.examples}}
            <pre class='api-code api-response-example' data-title='{{#starts-with content title}}Example Error{{else}}{{title}}{{/starts-with}}'><code class='prettyprint lang-{{type}}'>{{{content}}}</code></pre>
        {{/each}}

		<div class='api-op-footer'></div>
	{{/each}}

{{/with}}
{{/prettify}}
