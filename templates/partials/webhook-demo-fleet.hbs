<div class="webhookDemo apiHelper" data-control="fleet">
    <div class="apiHelperBox">
        <div class="showWhenStarted" style="display:none">
            <table class="apiHelperTableNoMargin">
                <thead id="fleetHeader">
                    <tr>
                        <th data-key="name">Name</th>
                        <th data-key="id">Device ID</th>
                        <th data-key="online" data-style="greenCheck">Online</th>
                        {{#if deviceOsVersion}}<th data-key="system_firmware_version">Device OS Version</th>{{/if}}
                        {{#if groups}}<th data-key="groups">Groups</th>{{/if}}
                    </tr>
                </thead>
                <tbody id="fleetTable">
                </tbody>
            </table>
        </div>
        <div class="hideWhenStarted">
            <p><a class="startDemo">Start Demo</a> to see the devices in your product.</p>
        </div>
    </div>
</div>
