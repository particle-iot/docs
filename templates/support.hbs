</head>
<body role='flatdoc'>
  <div id="docs" data-level>
    {{> header}}

    <div class='content-root'>
      <div class='menubar'>

      <ul class="nav reference">
        {{#each support}}
            <ul>
              <li class="top-level {{#page-equal ../path.href ../path.name path.href path.name}}active{{/page-equal}}">
                {{#page-equal ../path.href ../path.name path.href path.name}}
                  <span>{{{title}}}</span>
                  {{#if ../../toc}}
                    <i id="toggle-in-page-nav" class="ion-plus"></i>
                  {{/if}}
                {{else}}
                  <a href="{{path.href}}{{path.name}}">{{{title}}}</a>
                {{/page-equal}}
              </li>
                {{#page-equal ../path.href ../path.name path.href path.name}}
                  {{#if ../../toc}}
                    <div class="in-page-toc-container">
                      <ul class="nav in-page-toc hide">
                        {{#each ../../../toc}}
                        <li class="middle-level" data-nav="{{dataHref}}">{{#if children}}<i class="toggle-secondary-toc ion-arrow-right-b"></i>{{/if}}<a href="{{id}}" data-menu-href="{{dataHref}}">{{{text}}}</a></li>
                        {{#if children}}
                          <ul class="nav secondary-in-page-toc">
                            {{#each children}}
                              <li data-secondary-nav="{{dataHref}}"><a href="{{id}}" data-menu-href="{{dataHref}}">{{{text}}}</a></li>
                            {{/each}}
                          </ul>
                        {{/if}}
                        {{/each}}
                      </ul>
                    </div>
                  {{/if}}
                {{/page-equal}}
            </ul>
        {{/each}}
      </ul>
      </div>
      <div class="page-body">
        <div class='content-inner'>
          <div class="content" >
            {{{contents}}}
          </div>
        </div>
      </div>
    </div>

    {{> scripts}}
    <script type="text/javascript" src="//assets.zendesk.com/external/zenbox/v2.6/zenbox.js"></script>

    <style type="text/css" media="screen, projection">
    @import url(//assets.zendesk.com/external/zenbox/v2.6/zenbox.css);
    </style>
    <script type="text/javascript">
    if (typeof(Zenbox) !== "undefined") {
      Zenbox.init({
        dropboxID:   "20206950",
        url:         "https://particle.zendesk.com",
        tabTooltip:  "Support",
        tabImageURL: "https://p2.zdassets.com/external/zenbox/images/tab_support_right.png",
        tabColor:    "#00ADEF",
        hide_tab: true,
        tabPosition: "Right"
      });
    }
    </script>


</body>
</html>
